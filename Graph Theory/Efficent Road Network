n = 6
roads = [[3, 0], [0, 4], [5, 0], [2, 1],
          [1, 4], [2, 3], [5, 2]]

boo1 = False
list1 = list()
length = n - 1
counter1 = 0
while counter1 <= length:
    list1.append(counter1)
    counter1 = counter1 + 1

roads.sort()
    
    